<!DOCTYPE html>
<html>
<head>
  <title>Clinic</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag "active_admin", media: "all" %>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "application" %>
</head>
<body>
  <% if flash[:notice] %>
    <div class="flash_notice"><%= flash[:notice] %></div>
  <% end %>
  <% if flash[:alert] %>
    <div class="flash_alert"><%= flash[:alert] %></div>
  <% end %>
  <%= yield %>
</body>
</html>